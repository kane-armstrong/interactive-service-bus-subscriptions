<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <connectionStrings>
    <add name="AzureServiceBus" connectionString="" />
  </connectionStrings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd" autoReload="true" throwExceptions="true" internalLogLevel="Trace" internalLogFile="nlog-internal.log">
    <targets async="true">
      <target xsi:type="Debugger" name="debuggerLog" layout="${longdate} ${level:uppercase=true} ${callsite:className=true:includeSourcePath=false:methodName=true} ${message} ${exception:format=ToString}" />
      <target xsi:type="Console" name="consoleLog" layout="${longdate} ${level:uppercase=true} ${callsite:className=true:includeSourcePath=false:methodName=true} ${message} ${exception:format=ToString}" />
    </targets>
    <rules>
      <logger name="*" minLevel="Trace" writeTo="debuggerLog" />
      <logger name="*" minLevel="Trace" writeTo="consoleLog" />
    </rules>
  </nlog>
  <runtime>
